{
  "compilerOptions": {
    "target": "es2016",
    "lib": ["ES6", "DOM"],
    "outDir": "./dist",
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "module": "commonjs"
  },
  "include": ["ts/**/*.ts"],
  "exclude": ["node_modules"]
}
